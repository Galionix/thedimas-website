      "comps": [
        {
          "__component": "molecules.paragraph",
          "id": 82,
          "text": "\nIt was originally conceived as a simple application for quickly and conveniently organizing a list of tasks.",
          "types": null
        },
        {
          "__component": "molecules.paragraph",
          "id": 83,
          "text": "With time I added some features, and it became a little bit more complicated. ",
          "types": null
        },
        {
          "__component": "molecules.list",
          "id": 83,

          "name": "Dimas Planner features",
          "items": [
            {
              "name": "Theming options",
              "id": 2,
              "description": "Choose between light and dark themes for personal preference"
            },
            {
              "name": "View completed tasks",
              "id": 3,
              "description": "Allows viewing of completed todos for tracking and reference"
            },
            {
              "name": "Set task importance",
              "id": 4,
              "description": "Mark tasks as regular, urgent, or warning to prioritize"
            },
            {
              "name": "Highlight urgent tasks",
              "id": 5,
              "description": "Not completed urgent tasks are indicated in the main list with a distinct color for visibility"
            },
            {
              "name": "Track task time",
              "id": 6,
              "description": "Monitor creation and updated dates of tasks"
            },
            {
              "name": "Image upload and zoom",
              "id": 7,
              "description": "Upload images to tasks with a feature to zoom in on previews"
            },
            {
              "name": "Quick create/view today's list",
              "id": 8,
              "description": "Easily create or view a list for today with a single button at the top of the list"
            },
            {
              "name": "Mobile-ready PWA",
              "id": 9,
              "description": "Support for Progressive Web App, optimized for mobile use. You can add it to your home screen"
            },
            {
              "name": "Responsive mobile views",
              "id": 10,
              "description": "On mobile, easily hide, show, or wrap views with a swipe gesture"
            },
            {
              "name": "Shared lists with friends",
              "id": 11,
              "description": "Create and manage lists that can be shared with friends, allowing them to see and add tasks. Add friends by email"
            }
          ],

          "types": null
        },
        {
          "__component": "molecules.paragraph",
          "id": 84,
          "text": "This app ready to use",
          "types": null
        },
        {
          "__component": "atoms.link",
          "id": 85,
          "href": "https://planner.thedimas.com/",
          "label": "Planner",
          "target": "_blank"
        }
      ],
