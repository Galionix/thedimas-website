      {
        "__component": "molecules.paragraph",
        "id": 81,
        "text": "Galaktionov Dmitry: Crafting the Future of Web Development",
        "types": {
          "mobile": "title",
          "tablet": "title",
          "desktop": "title"
        }
      },
      {
        "__component": "molecules.paragraph",
        "id": 84,
        "text": "Welcome to My Digital Realm!",
        "types": {
          "mobile": "freaky",
          "tablet": "freaky",
          "desktop": "freaky"
        }
      },
      {
        "__component": "molecules.paragraph",
        "id": 82,
        "text": "As a passionate Full Stack Developer, I blend imagination and technology to shape exceptional web experiences. With over four years of professional experience, I've honed my skills in both front-end and back-end development, mastering technologies like React, Node.js, and Docker.",
        "types": null
